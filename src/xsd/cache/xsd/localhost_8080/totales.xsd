          <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="gIdExtType">
    <xs:sequence>

      <!--Numero de pasaporte o Identificación tributario(RUC)-->
      <xs:element name="dIdExt" minOccurs="1" maxOccurs="1">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <!--País-->
      <xs:element name="dPaisExt" minOccurs="0" maxOccurs="1">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="2"/>
            <xs:maxLength value="100"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="telefonoType">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{3,4}-[0-9]{4}"/>
      <!-- ajustar ficha tecnica-->
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="gGtinType">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0"/>
      <xs:totalDigits value="14"/>
      <xs:pattern value="([0]{1})|([0-9]{8})|([0-9]{12,14})"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="pesoType">
    <xs:restriction base="xs:decimal">
      <xs:pattern value="[0-9]{1,11}|[0-9]*[.][0-9]{0,4}"/>
      <xs:totalDigits value="11"/>
    </xs:restriction>
  </xs:simpleType>


</xs:schema>
