<?xml version="1.0" encoding="utf-8"?>
<xs:schema 
xmlns:xi="http://www.w3.org/2003/XInclude" 
xmlns="http://dgi-fep.mef.gob.pa" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ds="http://www.w3.org/2000/09/xmldsig#" id="FacturaElectronica" 
targetNamespace="http://dgi-fep.mef.gob.pa" elementFormDefault="qualified">



  <!--
  
        *************************************
  
            Grupo D
      
        *************************************
  
  -->

  <!--Subtotales y Totales de la Transacción-->
  <xs:complexType name="gTotType">
    <xs:annotation>
      <xs:documentation>
        Definición de tipo para el grupo:
        D01: Grupo con información de subtotales y totales
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>

      <xs:element name="dTotNeto" type="montoTipo01" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D02: Suma de los precios antes de impuesto
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dTotITBMS" type="montoTipo01" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D03: Total del ITBMS
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dTotISC" type="montoTipo01" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D04: Total del ISC
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dTotGravado" type="montoTipo01" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D05: Suma total de monto gravado
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dTotDesc" type="montoTipo01" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D06: Suma de los descuentos y bonificaciones concedidos sobre el valor total de la factura
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dTotAcar" type="montoTipo01" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D07: Valor del acarreo cobrado en el precio total
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dTotSeg" type="montoTipo01" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D08: Valor del seguro cobrado en el precio total
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dVTot" type="montoTipo01" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D09: Valor total de la factura
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dTotRec" type="montoTipo01" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D10: Suma de los valores recibidos
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dVuelto" type="montoTipo01" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D11: Vuelto entregado al cliente
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="iPzPag" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D12: Tiempo de pago
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:enumeration value="1">
              <xs:annotation>
                <xs:documentation>Inmediato</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
              <xs:annotation>
                <xs:documentation>Plazo</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
              <xs:annotation>
                <xs:documentation>Mixto</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="dNroItems" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D13: Número total de ítems de la factura
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:totalDigits value="11"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="dVTotItems" type="montoTipo01" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            D14: Suma total de los ítems con los montos de los impuestos
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="gDescBonif" minOccurs="0" maxOccurs="5">
        <xs:annotation>
          <xs:documentation>
            Definición de tipo para el grupo:
            D20: Grupo de datos de que describen descuentos o bonificaciones adicionales aplicados a la factura
          </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>

            <xs:element name="dDetalDesc">
              <xs:annotation>
                <xs:documentation>
                  D200: Descripción de descuentos o bonificaciones adicionales aplicados a la factura
                </xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:maxLength value="500"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <xs:element name="dValDesc" type="montoTipo01">
              <xs:annotation>
                <xs:documentation>
                  D201: Monto Descuentos/Bonificaciones y otros ajustes
                </xs:documentation>
              </xs:annotation>
            </xs:element>

          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <!-- Final de <xs:element name="gDescBonif">  -->

      <xs:element name="gFormaPago" minOccurs="1" maxOccurs="10">
        <xs:annotation>
          <xs:documentation>
            Definición de tipo para el grupo:
            D30: Grupo de formas de pago de la factura
          </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>

            <xs:element name="iFormaPago" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>
                  D301: Forma de pago de la factura
                </xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:enumeration value="01">
                    <xs:annotation>
                      <xs:documentation>Crédito</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="02">
                    <xs:annotation>
                      <xs:documentation>Contado</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="03">
                    <xs:annotation>
                      <xs:documentation>Tarjeta Crédito</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="04">
                    <xs:annotation>
                      <xs:documentation>Tarjeta Débito</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="05">
                    <xs:annotation>
                      <xs:documentation>Tarjeta Fidelidad</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="06">
                    <xs:annotation>
                      <xs:documentation>Vale devolución</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="07">
                    <xs:annotation>
                      <xs:documentation>Vale regalo</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
          <xs:enumeration value="08">
                    <xs:annotation>
                      <xs:documentation>Transf. / Depósito a cta. bancaria</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="99">
                    <xs:annotation>
                      <xs:documentation>Otro</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <xs:element name="dFormaPagoDesc" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>
                  D302: Descripción de forma de pago no listada en el formato
                </xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:annotation>
                    <xs:documentation>Obligatorio si existe D301(iFormaPago)</xs:documentation>
                  </xs:annotation>
                  <xs:minLength value="10"/>
                  <xs:maxLength value="100"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <xs:element name="dVlrCuota" type="montoTipo01" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>
                  D303: Valor de la fracción pagada utilizando esta forma de pago
                </xs:documentation>
              </xs:annotation>
            </xs:element>

          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <!-- Final de <xs:element name="gFormaPago">  -->

      <xs:element name="gRetenc" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Definición de tipo para el grupo:
            D40: Grupo datos cuando a la factura aplican retenciones
          </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="cCodRetenc" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>
                  D401: Código de Retención a aplicar
                </xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:totalDigits value="2"/>
                  <xs:enumeration value="1">
                    <xs:annotation>
                      <xs:documentation>Pago por servicio profesional al estado 100% 1</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="2">
                    <xs:annotation>
                      <xs:documentation>Pago por venta de bienes/servicios al estado 50%</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="3">
                    <xs:annotation>
                      <xs:documentation>Pago o acreditación a no domiciliado o empresa constituida en el exterior 100%</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="4">
                    <xs:annotation>
                      <xs:documentation>Pago o acreditación por compra de bienes/servicios 50%</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="7">
                    <xs:annotation>
                      <xs:documentation>Pago a comercio afiliado a sistema de TC/TD 50%</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="8">
                    <xs:annotation>
                      <xs:documentation>Otros (disminución de la retención)</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <xs:element name="cValRetenc" type="montoTipo01" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>
                  D402: Monto de la retención a aplicar
                </xs:documentation>
              </xs:annotation>
            </xs:element>

          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <!-- Final de <xs:element name="gRetenc">  -->

      <xs:element name="gPagPlazo" minOccurs="0" maxOccurs="99">
        <xs:annotation>
          <xs:documentation>
            Definición de tipo para el grupo:
            D50: Grupo de informaciones de pago a plazo
          </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>

            <xs:element name="dSecItem" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>
                  D501: Número secuencial de cada fracción de pago a plazo
                </xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:totalDigits value="2"/>
                  <xs:minInclusive value="00"/>
                  <xs:maxInclusive value="99"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <xs:element name="dFecItPlazo" type="fechaTZ" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>
                  D502: Fecha de vencimiento de la fracción
                </xs:documentation>
              </xs:annotation>
            </xs:element>

            <xs:element name="dValItPlazo" type="montoTipo01" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>
                  D503: Valor de la fracción
                </xs:documentation>
              </xs:annotation>
            </xs:element>

            <xs:element name="dInfPagPlazo" minOccurs="0" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>
                  D504: Informaciones de interés del emitente con respeto a  esta fracción de pago
                </xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:minLength value="15"/>
                  <xs:maxLength value="1000"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <!-- Final de <xs:element name="gPagPlazo">  -->


      <!--- Grupo de Total Otras Tasas o Impuestos (OTI) del Item -->
      <xs:element name="gOTITotal" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>D60:Grupo de Total Otras Tasas o Impuestos (OTI) del Item</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>

            <xs:element name="dCodOTITotal" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>D601:Código de Otras Tasas o Impuesto del total del ítem</xs:documentation>
              </xs:annotation>

              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:enumeration value="01">
                    <xs:annotation>
                      <xs:documentation>
                        SUMA911
                      </xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="02">
                    <xs:annotation>
                      <xs:documentation>
                        Tasa Portabilidad Numérica
                      </xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

            <xs:element name="dValOTITotal" type="montoTipo01" minOccurs="1" maxOccurs="1">
              <xs:annotation>
                <xs:documentation>D602:Monto de Otras Tasas o Impuesto del total del ítem</xs:documentation>
              </xs:annotation>
            </xs:element>

          </xs:sequence>
        </xs:complexType>

      </xs:element>


      <!-- Final de <xs:element name="gOTITotal">  -->


    </xs:sequence>
  </xs:complexType>
  <!-- Final de <xs:complexType name="gTotType">  -->


  <!--  *************************************
  
            GRUPO E
            
        *************************************
        
  -->
  <!--Vehículos Nuevos-->
  <xs:complexType name="gVehicNuevoType">

    <xs:sequence>

      <xs:element name="iModOpVN" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            E050: Modalidad de la operación de venta de vehículos nuevos
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="01">
              <xs:annotation>
                <xs:documentation>Venta a Representante</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="02">
              <xs:annotation>
                <xs:documentation>venta a consumidor final</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="03">
              <xs:annotation>
                <xs:documentation>venta a gobierno</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="04">
              <xs:annotation>
                <xs:documentation>venta a flota de vehículos</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="99">
              <xs:annotation>
                <xs:documentation>otra</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="dModOpVNDesc" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            E051: Modalidad de la operación de venta de vehículos nuevos no listada en el formato
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="10"/>
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="dChasi" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>E052:Chasis del vehículo</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="17"/>
            <xs:maxLength value="17"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="dColorCod" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>E053:Color del vehículo</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="4"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="dColorNomb" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>E054:Descripción del color del vehículo</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="40"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="dPotVeh" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>E055:Potencia del motor (CV)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:minInclusive value="1"/>
            <xs:totalDigits value="4"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="dCilin" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>E056:Capacidad del motor</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:minInclusive value="1"/>
            <xs:totalDigits value="4"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="dPesoNet" type="pesoType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>E057:Peso Neto</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="dPesoBruto" type="pesoType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>E058:Peso Bruto</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="iCombust" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>E060:Tipo de combustible</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="01">
              <xs:annotation>
                <xs:documentation>gasolina</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="02">
              <xs:annotation>
                <xs:documentation>diesel</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="03">
              <xs:annotation>
                <xs:documentation>etanol</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="08">
              <xs:annotation>
                <xs:documentation>electrico</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="09">
              <xs:annotation>
                <xs:documentation>gasolina/electico</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="99">
              <xs:annotation>
                <xs:documentation>otro</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="dCombustDesc" minOccurs="0" maxOccurs="1">
        <!--FICTEC: hacer cambio de nombre en ficha tecnica-->
        <xs:annotation>
          <xs:documentation>E061:Tipo de combustible no listado en el formato</xs:documentation>
        </xs:annotation>

        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="10"/>
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="dNroMotor" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>E062:Número del motor</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="21"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="dCapTracc" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>E063:Capacidad máxima de tracción</xs:documentation>
        </xs:annotation>

        <xs:simpleType>
          <xs:restriction base="xs:decimal">
            <xs:pattern value="[0-9]{1,11}|[0-9]*[.][0-9]{0,4}"/>
            <xs:totalDigits value="11"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <!---->
      <xs:element name="dEntreEj" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>E064:Distancia entre ejes</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:decimal">
            <xs:pattern value="[0-9]{1,11}|[0-9]*[.][0-9]{0,4}"/>
            <xs:totalDigits value="11"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <!---->
      <xs:element name="dAnoMod" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>E065:Año del modelo de fabricación</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:totalDigits value="4"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="dAnoFab" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>E067:Año de fabricación</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:totalDigits value="4"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="iTipoPintura" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>E068:Tipo de pintura</xs:documentation>
        </xs:annotation>

        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:enumeration value="1">
              <xs:annotation>
                <xs:documentation>sólida</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
              <xs:annotation>
                <xs:documentation>metálica</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
              <xs:annotation>
                <xs:documentation>perla</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
